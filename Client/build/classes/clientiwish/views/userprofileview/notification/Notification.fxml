<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="521.0" prefWidth="815.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientiwish.views.userprofileview.notification.NotificationController">
   <children>
      <TableView fx:id="notifTableView" layoutX="5.0" layoutY="114.0" prefHeight="395.0" prefWidth="802.0" stylesheets="@../../resources/userprofile/tableViewStyle.css">
        <columns>
          <TableColumn fx:id="itemNumber" prefWidth="135.0" text="Item Number" />
          <TableColumn fx:id="itemName" prefWidth="135.0" text="Item Name" />
            <TableColumn fx:id="itemPrice" prefWidth="136.0" text="Item Price" />
            <TableColumn fx:id="contributiors" prefWidth="203.0" text="Contributors" />
            <TableColumn fx:id="amounts" prefWidth="205.0" text="Amounts" />
        </columns>
      </TableView>
      <Button fx:id="bttnCollect" layoutX="311.0" layoutY="65.0" mnemonicParsing="false" onAction="#collectItem" prefHeight="37.0" prefWidth="193.0" text="Collect Item to my cart" />
      <Label fx:id="labelViewName" layoutX="68.0" layoutY="9.0" prefHeight="54.0" prefWidth="223.0" text="Notifications" textFill="#f6b63d">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <ImageView fx:id="iconImageView" fitHeight="53.0" fitWidth="47.0" layoutX="14.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/userprofile/bell.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
